@model IEnumerable<MediProjectWebApp.Models.Project>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>




<style>
    html, body {
        height: 100%;
    }
    body {
        margin: 0;
        height: 100vh;
        font-family: 'Roboto', sans-serif;
        color: #333;
        overflow: hidden;
        background: #4A90E2;
    }
</style>
<script>
    window.onpopstate = function (event) {
        $.ajax({
            type: 'GET',
            url: '@Url.Action("LogOut", "Login")'
        })
    };
    function pushState() {
        history.pushState(null, null, location.href);
    }

    window.onload = function () {
        pushState();
    };
</script>

